{
  "name": "tally-api-quickstart",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "gen": "run-p gen:*",
    "gen:types": "graphql-codegen --config codegen.ts",
    "gen:hooks": "esbuild autogen/schema.ts --format=esm --outfile=src/CodegenQuery/hooks.js",
    "dev": "run-p dev:*",
    "dev:types": "pnpm run gen:types -- --watch",
    "dev:hooks": "pnpm run gen:hooks -- --watch",
    "dev:vite": "vite",
    "build": "vite build"
  },
  "dependencies": {
    "@graphql-typed-document-node/core": "^3.2.0",
    "@tanstack/react-query": "^5.85.6",
    "graphql": "^16.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "viem": "^2.36.0",
    "wagmi": "^2.16.9",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^2.5.13",
    "@graphql-codegen/typescript-react-query": "^4.1.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "esbuild": "^0.17.5",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.32.2",
    "npm-run-all": "^4.1.5",
    "vite": "^4.0.0"
  }
}
